{
  "name": "nextaur",
  "version": "0.0.1",
  "description": "",
  "main": "server.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "nyc mocha test --recursive",
    "lint": "eslint *.js nac migrations/* server/* client/*.js",
    "build:templates": "cp client/templates/*.handlebars public/",
    "build:doc": "apidoc -i server/ -o public/apidoc/",
    "prebuild": "rimraf public/apidoc public/*.js public/*.css public/*.handlebars",
    "build": "npm run build:doc && npm run build:templates && lessc client/templates/*.less | uglifycss > public/style.css && browserify -t bulkify ./client/client.js | uglifyjs -mc > ./public/bundle.js",
    "debug": "npm run lint && npm run build:doc && npm run build:templates && lessc client/templates/*.less public/style.css && browserify -t bulkify ./client/client.js -o ./public/bundle.js",
    "start": "node server.js",
    "nodemon": "nodemon -e js,html,less,handlebars server.js"
  },
  "author": "Jan Houben",
  "license": "GPL-2.0",
  "devDependencies": {
    "apidoc": "^0.17.6",
    "browserify": "^14.4.0",
    "bulk-require": "^1.0.1",
    "bulkify": "^1.4.2",
    "eslint": "^4.2.0",
    "handlebars": "^4.0.10",
    "less": "^2.7.2",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "nyc": "^11.1.0",
    "rimraf": "^2.6.1",
    "uglify-es": "^3.0.25",
    "uglifycss": "0.0.27",
    "unit.js": "^2.0.0"
  },
  "dependencies": {
    "await-to-js": "^1.0.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "columnify": "^1.5.4",
    "commander": "^2.11.0",
    "dockerode": "^2.5.0",
    "express": "^4.15.3",
    "express-session": "^1.15.4",
    "glob": "^7.1.2",
    "knex": "^0.13.0",
    "nconf": "^0.8.4",
    "promptly": "^2.2.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "sqlite3": "^3.1.9",
    "tar-fs": "^1.15.3",
    "temp": "^0.8.3"
  }
}
